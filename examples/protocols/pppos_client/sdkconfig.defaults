CONFIG_EXAMPLE_MODEM_UART_TX_PIN=5
CONFIG_EXAMPLE_MODEM_UART_RX_PIN=13
# Override some defaults to enable PPP
CONFIG_LWIP_PPP_SUPPORT=y
CONFIG_LWIP_PPP_NOTIFY_PHASE_SUPPORT=y
CONFIG_LWIP_PPP_PAP_SUPPORT=y
CONFIG_LWIP_TCPIP_TASK_STACK_SIZE=4096
# Do not enable IPV6 in dte<->dce link local
CONFIG_LWIP_PPP_ENABLE_IPV6=n
CONFIG_EXAMPLE_MODEM_APN="CMNET"
CONFIG_EXAMPLE_MODEM_PPP_AUTH_USERNAME="www.coltsmart.com"
CONFIG_EXAMPLE_MODEM_PPP_AUTH_PASSWORD="coltsmart"
CONFIG_MQTT_TRANSPORT_SSL=n
CONFIG_ETH_ENABLED=n
CONFIG_UART_ISR_IN_IRAM=y
CONFIG_EXAMPLE_MODEM_UART_RX_BUFFER_SIZE=2048
CONFIG_EXAMPLE_MODEM_UART_TX_BUFFER_SIZE=2048
CONFIG_EXAMPLE_MODEM_UART_EVENT_QUEUE_SIZE=30
CONFIG_EXAMPLE_MODEM_UART_PATTERN_QUEUE_SIZE=20
CONFIG_EXAMPLE_MODEM_UART_EVENT_TASK_PRIORITY=22

#
# Modbus configuration
#
CONFIG_MB_UART_RXD=16
CONFIG_MB_UART_TXD=17
CONFIG_MB_UART_RTS=4
CONFIG_MB_UART_PORT_NUM=2
CONFIG_MB_COMM_MODE_RTU=y
CONFIG_MB_UART_BAUD_RATE=9600
CONFIG_FMB_TIMER_PORT_ENABLED=y
CONFIG_FMB_TIMER_GROUP=0
CONFIG_FMB_TIMER_INDEX=0
CONFIG_FMB_TIMER_ISR_IN_IRAM=y
CONFIG_FMB_MASTER_DELAY_MS_CONVERT=200
CONFIG_FMB_MASTER_TIMEOUT_MS_RESPOND=200

CONFIG_LOG_DEFAULT_LEVEL_INFO=y

#
# Partition Table
#
# CONFIG_PARTITION_TABLE_SINGLE_APP is not set
CONFIG_PARTITION_TABLE_TWO_OTA=y
# CONFIG_PARTITION_TABLE_CUSTOM is not set
CONFIG_PARTITION_TABLE_CUSTOM_FILENAME="partitions.csv"
CONFIG_PARTITION_TABLE_FILENAME="partitions_two_ota.csv"
CONFIG_PARTITION_TABLE_OFFSET=0x8000
CONFIG_PARTITION_TABLE_MD5=y
# end of Partition Table

# Default sdkconfig parameters to use the OTA
# partition table layout, with a 4MB flash size
CONFIG_ESPTOOLPY_FLASHSIZE_4MB=y
CONFIG_PARTITION_TABLE_TWO_OTA=y

#
# ESP HTTPS OTA
#
CONFIG_OTA_ALLOW_HTTP=y

# end of ESP HTTPS OTA
CONFIG_APP_PROJECT_VER_FROM_CONFIG=y
CONFIG_APP_PROJECT_VER="1.0"
CONFIG_ESP_TIMER_PROFILING=y